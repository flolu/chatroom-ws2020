{
  "name": "chat",
  "repository": "git@github.com/flolu/chatroom-ws2020.git",
  "private": true,
  "scripts": {
    "bazel": "bazelisk",
    "build": "bazel build //...",
    "user-client:dev": "ibazel run //services/user-client:dev_server",
    "user-client": "bazel run //services/user-client:app_server",
    "server-client:dev": "ibazel run //services/server-client:dev_server",
    "server-client": "bazel run //services/server-client:app_server",
    "patch": "patch-package --patch-dir dev/patches",
    "postinstall": "yarn run patch && ngcc"
  },
  "dependencies": {
    "@angular/common": "^11.0.7",
    "@angular/core": "^11.0.7",
    "@angular/forms": "^11.0.7",
    "@angular/platform-browser": "^11.0.7",
    "@angular/platform-browser-dynamic": "^11.0.7",
    "@angular/router": "^11.0.7",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular/compiler": "^11.0.7",
    "@angular/compiler-cli": "^11.0.7",
    "@bazel/bazelisk": "^1.6.1",
    "@bazel/concatjs": "^3.0.0",
    "@bazel/ibazel": "^0.13.2",
    "@bazel/rollup": "^3.0.0",
    "@bazel/terser": "^3.0.0",
    "@bazel/typescript": "^3.0.0",
    "@types/node": "^14.11.2",
    "history-server": "^1.3.1",
    "html-insert-assets": "^0.14.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^5.3.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "semi": false
  }
}
