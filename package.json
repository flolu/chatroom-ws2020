{
  "name": "chatroom-ws2020",
  "repository": "git@github.com/flolu/chatroom-ws2020.git",
  "private": true,
  "scripts": {
    "bazel": "bazelisk",
    "build": "bazel build //...",
    "client:dev": "ibazel run //services/client:dev_server",
    "server:dev": "bazelisk build //libs/config:generate_docker_compose_env && docker-compose -f docker-compose.yaml up --build",
    "patch": "patch-package --patch-dir dev/patches",
    "postinstall": "yarn run patch && ngcc"
  },
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/common": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@ngrx/effects": "9.1.2",
    "@ngrx/entity": "9.1.2",
    "@ngrx/router-store": "^9.1.2",
    "@ngrx/store": "9.1.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular/compiler": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@bazel/bazelisk": "^1.6.1",
    "@bazel/concatjs": "^3.0.0",
    "@bazel/ibazel": "^0.13.2",
    "@bazel/protractor": "^3.0.0",
    "@bazel/rollup": "^3.0.0",
    "@bazel/terser": "^3.0.0",
    "@bazel/typescript": "^3.0.0",
    "@ngrx/store-devtools": "9.1.2",
    "@types/node": "^14.11.2",
    "history-server": "^1.3.1",
    "html-insert-assets": "^0.14.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.1.2",
    "requirejs": "^2.3.6",
    "rollup": "^2.28.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^5.3.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "semi": false
  }
}
